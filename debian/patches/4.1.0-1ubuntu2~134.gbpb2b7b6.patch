+                    num=i)
+            if name == 'Point2':
+                var_arg_wrap += ' && size(varargin{{{num}}},1)==2'.format(
+                    num=i)
+                var_arg_wrap += ' && size(varargin{{{num}}},2)==1'.format(
+                    num=i)
