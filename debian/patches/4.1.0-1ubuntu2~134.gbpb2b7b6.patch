+            elif extra == 'deserialize':
+                body += self.wrap_collector_function_deserialize(
+                    collector_func[1].name,
+                    full_name=collector_func[1].cpp_class(),
+                    namespace=collector_func[0])
+            elif is_method or is_static_method:
