+            """).format(
+                class_name=namespace_name + '.' + instantiated_class.name,
+                wrapper=self._wrapper_name(),
+                id=self._update_wrapper_id(
+                    (namespace_name, instantiated_class, 'string_deserialize',
+                     'deserialize'))),
+                                           prefix='  ')
